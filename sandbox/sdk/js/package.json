{
  "name": "@agent-infra/sandbox",
  "version": "1.0.5",
  "description": "Node.js SDK for AIO Sandbox integration providing tools and interfaces",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rslib build && tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agent-infra/sandbox-sdk.git",
    "directory": "js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "aio",
    "sandbox",
    "sdk",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rslib/core": "^0.17.1",
    "@types/node": "^20.19.24",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
